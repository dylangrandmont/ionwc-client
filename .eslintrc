{
    "rules":{
        "block-spacing": 2,
        "brace-style": 2,
        "camelcase": 1,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "computed-property-spacing": 2,
        "curly": 2,
        "eqeqeq": 2,
        "func-call-spacing": 2,
        "function-paren-newline": 2,
        "multiline-comment-style": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 1,
        "no-useless-return": 2,
        "object-curly-spacing": 2,
        "operator-assignment": 2,
        "semi": 2,
        "semi-spacing": 2,
        "semi-style": 2,
        "space-in-parens": 2,
        "quotes": 0,
        "vars-on-top": 1 // Make this a 2
    },
    "parserOptions": {
      "ecmaVersion": 6
  }
}